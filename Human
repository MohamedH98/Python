class Human:
	def __init__(self, first, last, age):
		self.first = first
		self.last = last
		while age <= 0:
			raise ValueError("age cant be negative")

		self._age = age
			

	@property
	def age(self):
		return self._age

	@age.setter
	def age(self, value):
		if value >= 0:
			self._age = value
		else:
			raise ValueError("age cant be negative")

	@property
	def full_name(self):
		return f"{self.first} {self.last}"

	@full_name.setter
	def full_name(self, name):
		self.first, self.last = name.split(" ")

jane= Human("Jane", "Goodall", 34)
